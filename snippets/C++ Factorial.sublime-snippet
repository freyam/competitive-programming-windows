<snippet> <content><![CDATA[
ll fact(ll n) {
    ll res = 1;
    for (ll i = 2; i <= n; i++) res = res * i;
    return res;
}

ll nPr(ll n, ll r) {
    return fact(n)/fact(r); 
}

ll nCr(ll n, ll r) {
    return fact(n)/(fact(r)*fact(n - r)); 
}


]]></content> 
	<tabTrigger>fact</tabTrigger>
</snippet>
