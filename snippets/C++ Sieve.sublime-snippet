<snippet>
	<content><![CDATA[
bool prime[1000005];
vector<ll> nthprime;
void sieve() {
	ll n = 1000005;
	memset(prime,true,n);
	prime[0] = prime[1] = false;

	for (ll i = 2 ; i*i <= n ; ++i)
		if (prime[i])
			for (ll j = i*i ; j <= n ; j += i)
				prime[j] = false;

	for (ll i = 2 ; i <= n ; ++i)
		if (prime[i]) nthprime.pb(i);
}
]]></content>
	<tabTrigger>sieve</tabTrigger>
</snippet>
