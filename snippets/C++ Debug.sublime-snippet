	<content><![CDATA[
#define dx(x) cout << #x << " > " << x << endl
#define dv(v) cout << #v << " > "; \
	for(auto x : v) cout << x << " "; cout << endl
#define dm(m) cout << #m << endl;  \
	for(auto x : m) cout << x.first << " > " << x.second << endl

